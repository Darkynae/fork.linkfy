generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model links {
  id         BigInt    @id(map: "urls_pkey") @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  url        String?
  shorted    String?
}
